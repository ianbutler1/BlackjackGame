public class ShoeTester 
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        System.out.println("How many decks would you like to add to your shoe?");
        int num = sc.nextInt();
        Shoe s = new Shoe(num);
        System.out.println(s.getShoe());
        System.out.println("-----SHUFFLE--------------------");
        s.shuffle();
        System.out.println(s.getShoe());
        s.removeCard();
        System.out.println(s.getShoe());
        System.out.println(s.toString());
    }
}
