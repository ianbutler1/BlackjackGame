public class DeckTester 
{
    public static void main(String[] args)
    {
        Deck d = new Deck();
        System.out.println(d.toString());
        System.out.println(d.drawCard());
        System.out.println(d.drawCard());
        System.out.println(d.drawCard());
        System.out.println(d.drawCard());
        System.out.println(d.drawCard());
        System.out.println(d.drawCard());
        System.out.println(d.drawCard());
    }
}
